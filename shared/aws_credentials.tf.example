variable "aws_region" {
	default = "eu-west-1"
}

variable "aws_access" {
	default = "xxxxxxxxxxxxxxxxxxxx"
}

variable "aws_secret" {
	default = "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
}

provider "aws" {
	region 		= "${var.aws_region}"
	access_key	= "${var.aws_access}"
	secret_key	= "${var.aws_secret}"
}
